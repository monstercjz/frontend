{
  "rewrites": [
    {
      "source": "/simple-test",
      "destination": "https://www.google.com" 
    },
    
    {
      "source": "/test-env-param",
      "destination": "YOUR_WEBHOOK_SITE_URL?my_var=${TEST_VAR}"
    },
    {
      "source": "/api/:match*",
      "destination": "${BACKEND_API_BASE_URL}/api/:match*"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}